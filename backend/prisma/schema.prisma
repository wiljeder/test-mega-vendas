generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int    @id @default(autoincrement())
  name           String
  username       String @unique
  hashedPassword String
}

model Groups {
  id             Int        @id @default(autoincrement())
  name           String
  GroupsContacts Contacts[]
}

model Contacts {
  id      Int    @id @default(autoincrement())
  name    String
  phone   String
  groupId Int
  Groups  Groups @relation(fields: [groupId], references: [id])
}
